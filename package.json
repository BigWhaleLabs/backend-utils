{
  "name": "@big-whale-labs/backend-utils",
  "version": "0.0.13",
  "description": "Backend utils",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigWhaleLabs/backend-utils"
  },
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "np": {
    "2fa": false,
    "tests": false,
    "cleanup": false,
    "yarn": false,
    "contents": "dist"
  },
  "scripts": {
    "release": "np --no-publish",
    "prepare": "ts-patch install -s",
    "develop": "yarn prepare && tsc-watch --skipLibCheck --onSuccess 'node dist/index.js'",
    "build": "yarn prepare && tsc --skipLibCheck",
    "test": "yarn jest --forceExit --runInBand",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.0",
    "koa": "^2.13.4",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/koa": "^2.13.4",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^27.5.1",
    "np": "^7.6.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-patch": "^2.0.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "packageManager": "yarn@3.1.0"
}
