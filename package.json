  "files": [
    "dist"
  ],
  "np": {
    "2fa": false,
    "tests": false,
    "cleanup": false,
    "yarn": false,
    "contents": "dist"
  },
  "scripts": {
    "release": "np --no-publish",
    "distribute": "yarn build && node dist/index.js",
