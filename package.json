    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "amala": "^7.0.0",
    "class-validator-jsonschema": "^3.1.0",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "module-alias": "^2.2.2",
    "typescript": "^4.4.3"
